<program_description>
	<program provider="RXMCDA" name="additiveRank" displayName="additiveRank" version="1.0" />
	
	<documentation>
		<description>
			Determines ranking intervals for the given DMUs (alternatives) using additive Data Envelopment Analysis Model.
		</description>
		<contact>
			Malgorzata Napieraj (napieraj.malgorzata@gmail.com) 
		</contact>
		<reference>
			Gouveia M. C., Dias L. C., Antunes C. H., Additive DEA based on MCDA with imprecise information (2008). 
		</reference>
	</documentation>
	
	<parameters>
		<input id="alternatives" name="alternatives" displayName="alternatives" isoptional="0">
            <documentation>
                <description>A list of alternatives (DMUs).</description>
            </documentation>
            <xmcda tag="alternatives">
                <![CDATA[
                    <alternatives>
                        <alternative>
                            [...]
                        </alternative>
                        [...]
                    </alternatives>
                    ]]>
            </xmcda>
        </input>
		
		<input id="criteria" name="criteria" displayName="criteria" isoptional="0">
            <documentation>
                <description>A list of criteria with specified preference direction. List has to contains at least one criterion that will be minimized (input) and at least one criterion that will be maximized (output). In addition, minimum and maximum possible value for each critetion can be defined. If not defined, they will be computed from alternatives performances on given criteria. </description>
            </documentation>
            <xmcda tag="criteria">
                <![CDATA[
                    <criteria>
                        <criterion>
							<scale>
								<quantitative>
									<preferenceDirection>
										[...]
									</preferenceDirection>
								</quantitative>
							</scale>
							<minimum> [...] </minimum>
							<maximum> [...] </maximum>
                            [...]
                        </criterion>
                        [...]
                    </criteria>
                    ]]>
            </xmcda>
        </input>	

		<input id="performanceTable" name="performanceTable" displayName="performanceTable" isoptional="0">
            <documentation>
                <description>A list of alternatives (DMUs) performances. </description>
            </documentation>
            <xmcda tag="performanceTable">
                <![CDATA[
					<performanceTable>
						<alternativePerformances>
							<alternativeID> [...] </alternativeID>
							<performance>
								<criterionID>
									[...]
								</criterionID>
								<value>[...]</value>
							</performance>
							[...]
						</alternativePerformances>
						[...]
					</performanceTable>
                    ]]>
            </xmcda>
        </input>		
		
		<input id="criteriaLinearConstraints" name="criteriaLinearConstraints" displayName="criteriaLinearConstraints" isoptional="defaultFalse">
            <documentation>
                <description>A list of criteria linear constraints. </description>
            </documentation>
            <xmcda tag="criteriaLinearConstraints">
                <![CDATA[
					<criteriaLinearConstraints>
						<constraint>
							<element>
								<criterionID> [...] </criterionID>
								<coefficient>
									[...]
								</coefficient>
							</element>
							[...]
							<operator>[...]</operator>
							<rhs>
								[...]
							</rhs>
						</constraint>
						[...]
					</criteriaLinearConstraints>
                    ]]>
            </xmcda>
        </input>
		
		<input id="methodParameters" name="methodParameters" displayName="methodParameters" isoptional="defaultFalse">
            <documentation>
                <description>There are two possible method parameters -- "boundariesProvided" and "withConstraints". First one indicates if criteria values boundaries are given in criteria input or they have to be computed based on alternatives performances.   
				Second paramterer determines if criteria linear constraints will be taken under consideration.</description>
            </documentation>
            <xmcda tag="methodParameters">
                <![CDATA[
					<methodParameters>
							<parameter name="withConstraints">
								<value><integer>%1</integer></value>
							</parameter>
							<parameter name="boundariesProvided">
								<value><integer>%2</integer></value>
							</parameter>
					</methodParameters>
                    ]]>
            </xmcda>
			<gui status="preferFile">
			   <entry id="%1" type="int" displayName="withConstraints">
				  <constraint>
					 <description>The value should be an integer.</description>
				  </constraint>
				  <defaultValue>0</defaultValue>
				  <items>
					<item id="0">
						<description> Disactivated </description>
						<value>0</value>
					</item>
					<item id="not0">
						<description> Activated </description>
						<value>other than 0</value>
					</item>
				  </items>
			   </entry>
			   
			   <entry id="%2" type="int" displayName="boundariesProvided">
				  <constraint>
					 <description>The value should be an integer.</description>
				  </constraint>
				  <defaultValue>0</defaultValue>
				  <items>
					<item id="0">
						<description> Boundaries will be computed from alternatives performances. </description>
						<value>0</value>
					</item>
					<item id="not0">
						<description> Boundaries are given in criteria input </description>
						<value>other than 0</value>
					</item>
				  </items>
			   </entry>
			</gui>		
        </input>
		
		<output id="messages" name="messages" displayName="messages">
            
            <documentation>
                <description>A list of messages generated by the algorithm.</description>
            </documentation>
            <xmcda tag="methodMessages"/>
        </output>
        
       <output id="worstRank" name="worstRank" displayName="worstRank">
            <documentation>
				<description>
					A list of the worst rankings for given alternatives. 
				</description>
			</documentation>
            
           <xmcda tag="alternativesValues">
                <![CDATA[
					<alternativesValues mcdaConcept="worstRank">
						<alternativeValue>
						  <alternativeID> [...] </alternativeID>
						  <value>
							[...]
						  </value>
						</alternativeValue>
						[...]
					</alternativesValues>
                    ]]>
            </xmcda>
        </output>
		
		<output id="bestRank" name="bestRank" displayName="bestRank">
            <documentation>
				<description>
					A list of the best rankings for given alternatives. 
				</description>
			</documentation>
            
           <xmcda tag="alternativesValues">
                <![CDATA[
					<alternativesValues mcdaConcept="bestRank">
						<alternativeValue>
						  <alternativeID> [...] </alternativeID>
						  <value>
							[...]
						  </value>
						</alternativeValue>
						[...]
					</alternativesValues>
                    ]]>
            </xmcda>
        </output>
		
	</parameters>
	
</program_description>